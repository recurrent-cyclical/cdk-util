{
  "name": "app",
  "version": "0.1.1",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test-synth": "export CFN_VERSION=$npm_package_version && rm output/*.template.json ;  npm run lint && cdk synth --output ./output"
  },
  "devDependencies": {
    "aws-cdk": "^1.174.0",
    "@aws-cdk/assert": "^1.6.1",
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "typescript": "~3.6.2"
  },
  "dependencies": {
    "@aws-cdk/aws-sns": "^1.13.1",
    "@aws-cdk/aws-sns-subscriptions": "^1.13.1",
    "@aws-cdk/aws-sqs": "^1.13.1",
    "@aws-cdk/core": "^1.13.1"
  }
}
